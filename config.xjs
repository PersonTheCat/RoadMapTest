
// The number of chunks to display vertically.
chunkHeight: 32

// This number of chunks to display horizontally.
chunkWidth: 40

// The maximum y-coordinate to generate.
maxY: 128

// The minimum y-coordinate to generate.
minY: -48

// The depth of the added heightmap overlay.
grooveSize: 16

// Noise frequency for the main noise map.
frequency: 0.007

// Frequency for the groove noise.
grooveFrequency: 0.1

// The number of y-levels to render as the same color.
resolution: 12

